{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "outDir": "build/",
        "removeComments": true,
        "alwaysStrict": true
    },
    "typedocOptions": {
    	"entryPoints": ["src/index.ts", "src/ui.ts", "src/game.ts", "src/ants.ts"],
	"out": "docs",
	"excludeExternals": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
